generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL") // URL do pool (pgbouncer) do Supabase
  schemas  = ["aura"] // Esquemas a serem utilizados
}

model User {
  chatId      BigInt @id // chatId do Telegram
  name        String
  phoneNumber String @unique
  cpf         String @unique

  @@schema("aura")
  @@map("user")
}
